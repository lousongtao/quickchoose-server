        {
            "type": "basicfunction",
            "reference": {
                "name": "items",
                "type": "array"
            },
            "codeClass": null,
            "userConfig": {
                "designer|params": [
                    "record"
                ],
                "fn": "setDishSpecial",
                "implHandler": [
                    "\r",
                    "\r",
                    "var values = {\r",
                    "    userId : Ext.util.Cookies.get('userId'),\r",
                    "    sessionId : Ext.util.Cookies.get('sessionId'),\r",
                    "    id : record.get('objectid'),\r",
                    "    isSpecial : !record.get('isSpecial')\r",
                    "};\r",
                    "\r",
                    "var successCallback = function(resp, opts){\r",
                    "    var result = Ext.decode(resp.responseText);\r",
                    "    if (result.result == 'ok'){\r",
                    "        record.set('isSpecial', !record.get('isSpecial'));\r",
                    "        if (record.get('isSpecial')){\r",
                    "            record.set('displayText', record.get('displayText') + \"<html><font color=red>[SPECIAL]</font></html>\");\r",
                    "        } else {\r",
                    "            record.set('displayText', record.get('displayText').replace(\"[SPECIAL]\",\"\"));\r",
                    "        }\r",
                    "\r",
                    "\r",
                    "        Ext.Msg.alert(\"SUCCESS\",\"Done successfully.\");\r",
                    "    } else if (result.result == 'invalid_session'){\r",
                    "        digitalmenu.getApplication().onSessionExpired();\r",
                    "    } else {\r",
                    "        Ext.Msg.alert('failed to do, please do again');\r",
                    "    }\r",
                    "};\r",
                    "\r",
                    "var failureCallback = function(resp, opts){\r",
                    "    Ext.Msg.alert('failed to do, please do again');\r",
                    "};\r",
                    "\r",
                    "var confirmmsg = \"Do you want to set \" + record.get('chineseName') + \" Special?\";\r",
                    "if (record.get('isSpecial')){\r",
                    "    confirmmsg = \"Do you want to cancel Special of \" + record.get('chineseName') + \"?\";\r",
                    "}\r",
                    "Ext.Msg.confirm(\"Confirm\", confirmmsg,\r",
                    "                function(btnId){\r",
                    "                    if (btnId === 'no'){\r",
                    "                        return;\r",
                    "                    }\r",
                    "                    Ext.Ajax.request({\r",
                    "                        url: \"menu/change_dish_special\",\r",
                    "                        params : values,\r",
                    "                        success : successCallback,\r",
                    "                        failure : failureCallback\r",
                    "                    });\r",
                    "                });"
                ]
            },
            "designerId": "6531c9b8-6c13-4672-a7e5-d23d8af114a4"
